@Article{Fedorov2010,
  Title                    = {{Tropical cyclones and permanent El Ni\~{n}o in the early Pliocene epoch.}},
  Author                   = {Fedorov, Alexey V and Brierley, Christopher M and Emanuel, Kerry},
  Journal                  = {Nature},
  Year                     = {2010},

  Month                    = feb,
  Number                   = {7284},
  Pages                    = {1066--70},
  Volume                   = {463},

  Abstract                 = {Tropical cyclones (also known as hurricanes and typhoons) are now believed to be an important component of the Earth's climate system. In particular, by vigorously mixing the upper ocean, they can affect the ocean's heat uptake, poleward heat transport, and hence global temperatures. Changes in the distribution and frequency of tropical cyclones could therefore become an important element of the climate response to global warming. A potential analogue to modern greenhouse conditions, the climate of the early Pliocene epoch (approximately 5 to 3 million years ago) can provide important clues to this response. Here we describe a positive feedback between hurricanes and the upper-ocean circulation in the tropical Pacific Ocean that may have been essential for maintaining warm, El Ni\~{n}o-like conditions during the early Pliocene. This feedback is based on the ability of hurricanes to warm water parcels that travel towards the Equator at shallow depths and then resurface in the eastern equatorial Pacific as part of the ocean's wind-driven circulation. In the present climate, very few hurricane tracks intersect the parcel trajectories; consequently, there is little heat exchange between waters at such depths and the surface. More frequent and/or stronger hurricanes in the central Pacific imply greater heating of the parcels, warmer temperatures in the eastern equatorial Pacific, warmer tropics and, in turn, even more hurricanes. Using a downscaling hurricane model, we show dramatic shifts in the tropical cyclone distribution for the early Pliocene that favour this feedback. Further calculations with a coupled climate model support our conclusions. The proposed feedback should be relevant to past equable climates and potentially to contemporary climate change.},
  Doi                      = {10.1038/nature08831},
  File                     = {:Fedorov, Brierley, Emanuel - 2010.pdf:PDF},
  ISSN                     = {1476-4687},
  Keywords                 = {Atlantic Ocean,Atmosphere,Atmosphere: chemistry,Carbon Dioxide,Carbon Dioxide: analysis,Cyclonic Storms,Cyclonic Storms: history,Global Warming,History, Ancient,Hot Temperature,Pacific Ocean,Seawater,Time Factors,Tropical Climate,Water Movements,Wind},
  Owner                    = {tnauss},
  Pmid                     = {20182509},
  Publisher                = {Nature Publishing Group},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/20182509}
}

