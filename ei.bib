@Article{Doherty2010,
  Title                    = {{Implications of future climate and atmospheric CO 2 content for regional biogeochemistry, biogeography and ecosystem services across East Africa}},
  Author                   = {Doherty, Ruth M. and Sitch, Stephen and Smith, Benjamin and Lewis, Simon L. and Thornton, Philip K.},
  Journal                  = {Global Change Biology},
  Year                     = {2010},

  Month                    = feb,
  Number                   = {2},
  Pages                    = {617--640},
  Volume                   = {16},

  Abstract                 = {We model future changes in land biogeochemistry and biogeography across East Africa. East Africa is one of few tropical regions where general circulation model (GCM) future climate projections exhibit a robust response of strong future warming and general annual-mean rainfall increases. Eighteen future climate projections from nine GCMs participating in the Intergovernmental Panel on Climate Change (IPCC) Fourth Assess- ment were used as input to the LPJ dynamic global vegetation model (DGVM), which predicted vegetation patterns and carbon storage in agreement with satellite observations and forest inventory data under the present-day climate. All simulations showed future increases in tropical woody vegetation over the region at the expense of grasslands. Regional increases in net primary productivity (NPP) (18â€“36\%) and total carbon storage (3â€“13\%) by 2080â€“2099 compared with the present-day were common to all simulations. Despite decreases in soil carbon after 2050, seven out of nine simulations continued to show an annual net land carbon sink in the final decades of the 21st century because vegetation biomass continued to increase. The seasonal cycles of rainfall and soil moisture show future increases in wet season rainfall across the GCMs with generally little change in dry season rainfall. Based on the simulated present-day climate and its future trends, the GCMs can be grouped into four broad categories. Overall, our model results suggest that East Africa, a populous and economically poor region, is likely to experience some ecosystem service benefits through increased precipitation, river runoff and fresh water availability. Resulting enhancements in NPP may lead to improved crop yields in some areas. Our results stand in partial contradiction to other studies that suggest possible negative consequences for agriculture, biodiversity and other ecosystem services caused by temperature increases.},
  Doi                      = {10.1111/j.1365-2486.2009.01997.x},
  File                     = {:/Doherty et al. - 2010.pdf:PDF},
  ISSN                     = {13541013},
  Keywords                 = {2009, Biodiversity, Ecosystem Services, Kilimanjaro, and accepted 10 may, climate change, climate uncertainty, dgvm, dynamic global vegetation models, east africa, ecosystem services, general circulation models, received 29 january 2009, regional climate impacts},
  Owner                    = {tnauss},
  Tags                     = {Kilimanjaro project},
  Url                      = {http://doi.wiley.com/10.1111/j.1365-2486.2009.01997.x}
}

