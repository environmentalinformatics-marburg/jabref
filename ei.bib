@Article{Kug2009,
  Title                    = {{Two Types of El Ni\~{n}o Events: Cold Tongue El Ni\~{n}o and Warm Pool El Ni\~{n}o}},
  Author                   = {Kug, Jong-Seong and Jin, Fei-Fei and An, Soon-Il},
  Journal                  = {Journal of Climate},
  Year                     = {2009},

  Month                    = mar,
  Number                   = {6},
  Pages                    = {1499--1515},
  Volume                   = {22},

  Abstract                 = {In this study, two types of El NinËœo events are classified based on spatial patterns of the sea surface tem- perature (SST) anomaly. One is the cold tongue (CT) El NinËœo, which can be regarded as the conventional El NinËœo, and the other the warm pool (WP) El NinËœo. The CT El NinËœo is characterized by relatively large SST anomalies in the NinËœo-3 region (58Sâ€“58N, 1508â€“908W), while theWP El NinËœo is associated with SST anomalies mostly confined to the NinËœo-4 region (58Sâ€“58N, 1608Eâ€“1508W). In addition, spatial patterns of many atmo- spheric and oceanic variables are also distinctively different for the two types of El NinËœo events. Furthermore, the difference in the transition mechanism between the two types of El NinËœo is clearly identified. That is, the discharge process of the equatorial heat content associated with theWP El NinËœo is not efficient owing to the spatial structure of SST anomaly; as a result, it cannot trigger a cold event. It is also demonstrated that zonal advective feedback (i.e., zonal advection ofmean SST by anomalous zonal currents) plays a crucial role in the development of a decaying SST anomaly associated with theWP El NinËœo, while thermocline feedback is a key process during the CT El NinËœo.},
  Doi                      = {10.1175/2008JCLI2624.1},
  File                     = {:/Kug, Jin, An - 2009.pdf:PDF},
  ISSN                     = {0894-8755},
  Keywords                 = {Kilimanjaro},
  Owner                    = {tnauss},
  Tags                     = {Kilimanjaro project},
  Url                      = {http://journals.ametsoc.org/doi/abs/10.1175/2008JCLI2624.1}
}

